{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "config": [
        "./blog.config.ts"
      ],
      "types": [
        "./types/index.d.ts"
      ]
    }
  },
  "include": [
    "./blog.config.ts",
    "./types/index.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.astro"
  ]
}